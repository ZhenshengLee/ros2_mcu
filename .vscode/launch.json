{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "custom_stm32f767zi-stlink-dbg",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "stutil",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/libs/custom_stm32f767zi/build/micro-ROS.elf",
            "runToEntryPoint": "appMain",
            "armToolchainPath": "${workspaceRoot}/toolchain/gcc-arm-none-eabi-8-2019-q3-update/bin",
            "svdFile": "${workspaceRoot}/.vscode/svd/STM32F767.svd",
        },
        {
            "name": "host-gdb-run",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/app_ws/install/micro_ros_int32_publisherlib/micro_ros_int32_publisher/int32_publisher",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [
                {
                    "name": "GA_LOG_LEVEL",
                    "value": "t"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            // not available
            "name": "custom_stm32f767zi-openocd-dbg",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "openocd",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/libs/custom_stm32f767zi/build/micro-ROS.elf",
            "runToEntryPoint": "main",
            "armToolchainPath": "${workspaceRoot}/toolchain/gcc-arm-none-eabi-8-2019-q3-update/bin",
            "device": "STM32F767ZIT6",
            "svdFile": "${workspaceRoot}/.vscode/svd/STM32F767.svd",
            "configFiles": [
                "interface/stlink-v2-1.cfg",
                "target/stm32f7x.cfg"
            ],
            "rtos": "FreeRTOS",
        }
    ]
}