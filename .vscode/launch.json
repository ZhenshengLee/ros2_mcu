    "version": "0.2.0",
    "configurations": [
        {
            "name": "nucleo_stm32f767zi-micro-ROS-debug",
            "cwd": "${workspaceRoot}",
            "executable": "./libs/nucleo_stm32f767zi/build/microros-ROS.bin",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "armToolchainPath": "${workspaceRoot}/toolchain/gcc-arm-none-eabi-8-2019-q3-update/bin",
